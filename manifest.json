{
	"name": "ODU Google Calendar Importer",
	"version": "1.0",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"scripts": ["background.js"]
	}
}